<?xml version="1.0" encoding="UTF-8"?>
<?import cn.oyzh.fx.gui.svg.label.AboutSVGLabel?>
<?import cn.oyzh.fx.gui.svg.label.KeySVGLabel?>
<?import cn.oyzh.fx.gui.svg.label.MessageSVGLabel?>
<?import cn.oyzh.fx.gui.svg.label.QuitSVGLabel?>
<?import cn.oyzh.fx.gui.svg.label.SettingSVGLabel?>
<?import cn.oyzh.fx.gui.svg.label.SnippetSVGLabel?>
<?import cn.oyzh.fx.gui.svg.label.ToolsSVGLabel?>
<?import cn.oyzh.fx.gui.svg.label.TransportSVGLabel?>
<?import cn.oyzh.fx.gui.svg.pane.LayoutSVGPane2?>
<?import cn.oyzh.fx.plus.controls.box.FXHBox?>
<?import cn.oyzh.fx.plus.controls.FXHeaderBar?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<FXHeaderBar xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.oyzh.easyshell.controller.HeaderController2"
             prefHeight="30">
    <content>
        <FXHBox flexHeight="100%">
            <TransportSVGLabel  tipText="%base.transportFile" onMousePrimaryClicked="#transport" size="12">
                <FXHBox.margin>
                    <Insets left="10" top="7"/>
                </FXHBox.margin>
            </TransportSVGLabel>
            <KeySVGLabel tipText="%base.key1Manager" onMousePrimaryClicked="#key" size="13">
                <FXHBox.margin>
                    <Insets left="10" top="7"/>
                </FXHBox.margin>
            </KeySVGLabel>
            <SnippetSVGLabel tipText="%base.snippet" onMousePrimaryClicked="#snippet" size="13">
                <FXHBox.margin>
                    <Insets left="10" top="7"/>
                </FXHBox.margin>
            </SnippetSVGLabel>
            <MessageSVGLabel tipText="%base.message" onMousePrimaryClicked="#message" size="13">
                <FXHBox.margin>
                    <Insets left="10" top="7"/>
                </FXHBox.margin>
            </MessageSVGLabel>
            <ToolsSVGLabel tipText="%base.tool" onMousePrimaryClicked="#tool" size="13">
                <FXHBox.margin>
                    <Insets left="10" top="7"/>
                </FXHBox.margin>
            </ToolsSVGLabel>
            <AboutSVGLabel tipText="%base.about" onMousePrimaryClicked="#about" size="13">
                <FXHBox.margin>
                    <Insets left="10" top="7"/>
                </FXHBox.margin>
            </AboutSVGLabel>
            <LayoutSVGPane2 fx:id="layoutPane" onMousePrimaryClicked="#layout" size="12">
                <FXHBox.margin>
                    <Insets left="10" top="7"/>
                </FXHBox.margin>
            </LayoutSVGPane2>
            <SettingSVGLabel tipText="%base.setting" onMousePrimaryClicked="#setting" size="14">
                <FXHBox.margin>
                    <Insets left="10" top="7"/>
                </FXHBox.margin>
            </SettingSVGLabel>
            <QuitSVGLabel tipText="%base.quit" onMousePrimaryClicked="#quit" size="12">
                <FXHBox.margin>
                    <Insets left="10" top="7"/>
                </FXHBox.margin>
            </QuitSVGLabel>
        </FXHBox>
    </content>
</FXHeaderBar>
